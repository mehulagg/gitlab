- page_title _('Jira Issues')

- if Feature.enabled?(:vue_issuables_list, @project)
  .js-issuables-list{ data: { endpoint: expose_url(namespace_project_jira_issues_data_path(namespace_id: @project.group, project_id: @project)),
    'can-bulk-edit': false,
    'empty-svg-path': image_path('illustrations/issues.svg'),
    'sort-key': @sort } }
